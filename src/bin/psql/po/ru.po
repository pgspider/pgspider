#, c-format
msgid ""
"Command:     %s\n"
"Description: %s\n"
"Syntax:\n"
"%s\n"
"\n"
"URL: %s\n"
"\n"
msgstr ""
"Команда:     %s\n"
"Описание:    %s\n"
"Синтаксис:\n"
"%s\n"
"\n"
"URL: %s\n"
"\n"

